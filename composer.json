{
  "name": "pepeverde/gestpayws",
  "type": "library",
  "description": "PHP implementation of GestPay (Banca Sella) Web Services",
  "keywords": [
    "gestpay",
    "bancasella",
    "webservice"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "endelwar@aregar.it"
    }
  ],
  "support": {
    "issues": "https://github.com/pepeverde/GestPayWS/issues"
  },
  "autoload": {
    "psr-4": {
      "EndelWar\\GestPayWS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EndelWar\\GestPayWS\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-openssl": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5 || ^6 || ^7",
    "codacy/coverage": "~1.0",
    "php-coveralls/php-coveralls": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  }
}
